# NextAuth Configuration
NEXTAUTH_URL=http://localhost:3033
NEXTAUTH_SECRET=your-nextauth-secret-here

# Database
DATABASE_URL="postgresql://username:password@localhost:5432/database_name"

# OAuth2 Configuration
NEXT_PUBLIC_AUTHSYS_BASE_URL=https://your-auth-system.com
NEXT_PUBLIC_CLIENT_ID=your-client-id
CLIENT_SECRET=your-client-secret
NEXT_PUBLIC_REDIRECT_URI=http://localhost:3033/auth/callback
NEXT_PUBLIC_SCOPE=openid profile email

# Internal Auth System Base URL (for server-side calls)
AUTHSYS_BASE_URL=https://your-auth-system.com

# Development
NODE_ENV=development